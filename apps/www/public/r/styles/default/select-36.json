      "content": "import { Label } from \"@/components/ui/label\";\nimport {\n  Select,\n  SelectContent,\n  SelectItem,\n  SelectTrigger,\n  SelectValue,\n} from \"@/components/ui/select\";\n\nexport default function SelectDemo() {\n  return (\n    <div className=\"space-y-2\">\n      <Label htmlFor=\"select-36\">\n        Select with description and right indicator\n      </Label>\n      <Select defaultValue=\"s2\">\n        <SelectTrigger id=\"select-36\" className=\"[&_[data-desc]]:hidden\">\n          <SelectValue placeholder=\"Choose a plan\" />\n        </SelectTrigger>\n        <SelectContent className=\"[&_*[role=option]>span]:end-2 [&_*[role=option]>span]:start-auto [&_*[role=option]]:pe-8 [&_*[role=option]]:ps-2\">\n          <SelectItem value=\"s1\">\n            Standard Plan\n            <span\n              className=\"mt-1 block text-xs text-muted-foreground\"\n              data-desc\n            >\n              Ideal for individuals\n            </span>\n          </SelectItem>\n          <SelectItem value=\"s2\">\n            Pro Plan\n            <span\n              className=\"mt-1 block text-xs text-muted-foreground\"\n              data-desc\n            >\n              For professional users\n            </span>\n          </SelectItem>\n          <SelectItem value=\"s3\">\n            Enterprise Plan\n            <span\n              className=\"mt-1 block text-xs text-muted-foreground\"\n              data-desc\n            >\n              Built for large teams\n            </span>\n          </SelectItem>\n        </SelectContent>\n      </Select>\n    </div>\n  );\n}\n",
