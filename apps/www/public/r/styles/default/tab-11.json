      "content": "import { Box, House, PanelsTopLeft } from \"lucide-react\";\n\nimport { ScrollArea, ScrollBar } from \"@/components/ui/scroll-area\";\nimport { Tabs, TabsContent, TabsList, TabsTrigger } from \"@/components/ui/tabs\";\n\nexport default function TabDemo() {\n  return (\n    <Tabs defaultValue=\"tab-1\">\n      <ScrollArea>\n        <TabsList className=\"relative mb-3 h-auto w-full gap-0.5 bg-transparent p-0 before:absolute before:inset-x-0 before:bottom-0 before:h-px before:bg-border\">\n          <TabsTrigger\n            value=\"tab-1\"\n            className=\"overflow-hidden rounded-b-none border-x border-t border-border bg-muted py-2 data-[state=active]:z-10 data-[state=active]:shadow-none\"\n          >\n            <House\n              className=\"-ms-0.5 me-1.5 opacity-60\"\n              size={16}\n              strokeWidth={2}\n              aria-hidden=\"true\"\n            />\n            Overview\n          </TabsTrigger>\n          <TabsTrigger\n            value=\"tab-2\"\n            className=\"overflow-hidden rounded-b-none border-x border-t border-border bg-muted py-2 data-[state=active]:z-10 data-[state=active]:shadow-none\"\n          >\n            <PanelsTopLeft\n              className=\"-ms-0.5 me-1.5 opacity-60\"\n              size={16}\n              strokeWidth={2}\n              aria-hidden=\"true\"\n            />\n            Projects\n          </TabsTrigger>\n          <TabsTrigger\n            value=\"tab-3\"\n            className=\"overflow-hidden rounded-b-none border-x border-t border-border bg-muted py-2 data-[state=active]:z-10 data-[state=active]:shadow-none\"\n          >\n            <Box\n              className=\"-ms-0.5 me-1.5 opacity-60\"\n              size={16}\n              strokeWidth={2}\n              aria-hidden=\"true\"\n            />\n            Packages\n          </TabsTrigger>\n        </TabsList>\n        <ScrollBar orientation=\"horizontal\" />\n      </ScrollArea>\n      <TabsContent